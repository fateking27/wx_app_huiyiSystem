.menu_warp {
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  height: 200rpx;
  background-color: #fff;
}

.menu_item {
  text-align: center;
}

.menu_contents {
  width: 750rpx;
  background-color: #fff;
  margin-top: 25rpx;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
  flex-wrap: wrap;
}

.content {
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding-bottom: 10rpx;
  width: 750rpx;
  height: 200rpx;
  border-bottom: 1rpx solid #999;
}